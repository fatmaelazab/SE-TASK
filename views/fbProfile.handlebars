<h2 class="page-header" style="color:#F5F5F5"> Welcome {{user.facebook.name}}</h2>
{{#if errors}}
      {{#each errors}}
        <div class="alert alert-danger">{{msg}}</div>
      {{/each}}
{{/if}}
<style>
   body{
     background-image: url("images.jpeg");
     background-color:#2c3e50;
   }
</style>

<body>

   <div class="form-group" >
    <label>ID: {{user.facebook.id}}  </label>
    <input type="text" style="display:none;" class="form-control" placeholder="id" name="id">
  </div>

     <div class="form-group" >
    <label>Token: {{user.facebook.token}}</label>
    <input type="text" style="display:none;" class="form-control" placeholder="token" name="token">
  </div>

       <div class="form-group" >
    <label>Name: {{user.facebook.name}}</label>
    <input type="text" style="display:none;" class="form-control" placeholder="name" name="name">
  </div>
    <div class="form-group" >
    <label>Email: {{user.facebook.email}}</label>
    <input type="text" style="display:none;" class="form-control" placeholder="email" name="email">
  </div>
</body>
